{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "samples/sandbox",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/samples/Sandbox/bin/Debug/netcoreapp2.0/Sandbox.dll",
      "args": [],
      "cwd": "${workspaceFolder}/samples/Sandbox",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}